{
  "name": "wandb-viewer",
  "displayName": "Bread Wandb Viewer",
  "description": "View Weights & Biases run metrics and logs directly in VS Code",
  "version": "0.2.4",
  "publisher": "bread-tech",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bread-Technologies/bread_wandb_viewer_extension"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Visualization",
    "Machine Learning",
    "Data Science"
  ],
  "keywords": [
    "wandb",
    "weights and biases",
    "machine learning",
    "ml",
    "deep learning",
    "pytorch",
    "tensorflow",
    "training metrics",
    "visualization",
    "ai",
    "model training",
    "experiment tracking",
    "offline viewer",
    "protobuf"
  ],
  "activationEvents": [
    "onCustomEditor:wandb-viewer.wandbFile",
    "onCommand:wandb-viewer.compareRuns"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wandb-viewer.compareRuns",
        "title": "Bread Wandb Viewer",
        "category": "Bread"
      }
    ],
    "customEditors": [
      {
        "viewType": "wandb-viewer.wandbFile",
        "displayName": "Bread Wandb Viewer",
        "selector": [
          {
            "filenamePattern": "*.wandb"
          }
        ],
        "priority": "default"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "wandb-viewer.compareRuns",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "protobufjs": "^7.2.6",
    "@vscode/extension-telemetry": "^0.9.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.0.0"
  }
}
